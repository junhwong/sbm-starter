<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.springboot.starter.mybatis.mapper.CityMapper">
    <select id="findByState" resultType="org.springboot.starter.mybatis.domain.City">
        select * from city where state = #{state};
    </select>
</mapper>
